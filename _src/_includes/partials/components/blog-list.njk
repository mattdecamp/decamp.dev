{%- for blog in blogItems %}

<h3>{{ blog.date | dateTime }}</h3>
{%- endfor -%}
<ul class="article-list">
  {% for blog in blogs | reverse %}
  <li>
      <div>
          <h2>
            <a href="/blog/{{ blog.title | slug | url }}/">{{ blog.title }}</a>
          </h2>
          <time value="{{ blog.pubDate | dateTime }}">{{ blog.pubDate | fixDate }}</time>
          <p>
            {{ blog.excerpt }}
          </p>
      </div>
  </li>

  {%- endfor -%}
</ul>
