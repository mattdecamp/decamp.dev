{
  "name": "decampdev",
  "version": "1.0.0",
  "description": "Professional Site for Matt DeCamp",
  "main": ".eleventy.js",
  "scripts": {
    "serve": "npx @11ty/eleventy --serve",
    "scss": "node-sass --output-style compressed -o css scss",
    "autoprefixer": "postcss -u autoprefixer -r css/styles.css",
    "lint": "eslint",
    "babel": "babel js -d _site/js",
    "watch:css": "onchange 'scss/' -- npm run scss",
    "start": "run-p serve watch:css autoprefixer"
  },
  "keywords": [],
  "author": "Matt DeCamp",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-plugin-rss": "^1.1.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.2",
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "autoprefixer": "^10.3.1",
    "eleventy-plugin-excerpt": "^1.1.2",
    "eslint": "^7.32.0",
    "gray-matter": "^4.0.3",
    "luxon": "^2.0.2",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.3.2",
    "uglify-js": "^3.14.1"
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  }
}
