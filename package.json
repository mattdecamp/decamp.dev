{
  "name": "decampdev",
  "version": "1.0.0",
  "description": "Professional Site for Matt DeCamp",
  "main": ".eleventy.js",
  "scripts": {
    "serve": "npx @11ty/eleventy --serve",
    "scss": "node-sass --output-style compressed -o __src/css __src/scss",
    "autoprefixer": "postcss -u autoprefixer -r __src/css/styles.css",
    "lint": "eslint",
    "watch:css": "onchange '__src/scss/' -- npm run scss",
    "start": "run-p serve watch:css autoprefixer"
  },
  "keywords": [],
  "author": "Matt DeCamp",
  "repository": {
    "type": "git",
    "url": "git://github.com/mattdecamp/decampdev.git"
  },
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-img": "^1.0.0",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.3",
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "autoprefixer": "^10.4.0",
    "eleventy-plugin-excerpt": "^1.1.2",
    "eslint": "^8.2.0",
    "gray-matter": "^4.0.3",
    "luxon": "^2.1.1",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss-cli": "^9.0.2",
    "prettier": "^2.4.1",
    "uglify-js": "^3.14.3"
  },
