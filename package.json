    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "clean-css": "^5.2.4",
    "cross-env": "^7.0.3",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.0.0",
    "eleventy-plugin-svg-sprite": "^2.1.0",
    "html-minifier": "^4.0.0",
    "luxon": "^3.2.1",
    "markdown-it": "^12.3.2",
    "node-fetch": "^3.3.0",
